
def name_validator(user_name):
    non_name = "1234567890-=`!@#$%^&*()_+[]\{\}|\\/<>,.;:?"

    for i in user_name:
        for j in non_name:
            if i == j:
                print("ERROR because it contains ", j)
                return False
    return True
    
